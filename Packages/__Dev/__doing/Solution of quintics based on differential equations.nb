            RowBox[{"\[FormalY]", "[", "\[FormalX]", "]"}]}], "+", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"5", " ", "\[FormalX]"}], "-", 
              RowBox[{"5", " ", 
               SuperscriptBox["\[FormalX]", "3"]}], "+", 
