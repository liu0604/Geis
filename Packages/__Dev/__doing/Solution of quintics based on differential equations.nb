Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"sol", "=", 
  RowBox[{"DSolve", "[", 
   RowBox[{
    RowBox[{"diffeqn", "\[Equal]", "0"}], ",", 
    RowBox[{"t", "[", "\[Rho]", "]"}], ",", "\[Rho]"}], "]"}]}]], "Input",
