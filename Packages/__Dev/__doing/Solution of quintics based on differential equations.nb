Cell[BoxData[
 RowBox[{"{", 
  RowBox[{"{", 
   RowBox[{
    RowBox[{"t", "[", "\[Rho]", "]"}], "\[Rule]", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Rho]", " ", 
        RowBox[{"Root", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            SuperscriptBox["#1", "4"], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "3"], " ", 
