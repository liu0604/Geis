             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             SubscriptBox["a", "5"], 
             SubscriptBox["a", "1"]]}], "&"}], ",", "1"}], "]"}]}]], " ", 
      RowBox[{"C", "[", "1", "]"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Rho]", " ", 
        RowBox[{"Root", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            SuperscriptBox["#1", "4"], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "3"], " ", 
              SubscriptBox["a", "2"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "2"], " ", 
              SubscriptBox["a", "3"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{"#1", " ", 
              SubscriptBox["a", "4"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             SubscriptBox["a", "5"], 
             SubscriptBox["a", "1"]]}], "&"}], ",", "2"}], "]"}]}]], " ", 
      RowBox[{"C", "[", "2", "]"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Rho]", " ", 
        RowBox[{"Root", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            SuperscriptBox["#1", "4"], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "3"], " ", 
              SubscriptBox["a", "2"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "2"], " ", 
              SubscriptBox["a", "3"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{"#1", " ", 
              SubscriptBox["a", "4"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             SubscriptBox["a", "5"], 
             SubscriptBox["a", "1"]]}], "&"}], ",", "3"}], "]"}]}]], " ", 
      RowBox[{"C", "[", "3", "]"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{"\[Rho]", " ", 
        RowBox[{"Root", "[", 
         RowBox[{
          RowBox[{
           RowBox[{
            SuperscriptBox["#1", "4"], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "3"], " ", 
              SubscriptBox["a", "2"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{
              SuperscriptBox["#1", "2"], " ", 
              SubscriptBox["a", "3"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             RowBox[{"#1", " ", 
              SubscriptBox["a", "4"]}], 
             SubscriptBox["a", "1"]], "+", 
            FractionBox[
             SubscriptBox["a", "5"], 
             SubscriptBox["a", "1"]]}], "&"}], ",", "4"}], "]"}]}]], " ", 
      RowBox[{"C", "[", "4", "]"}]}], "-", 
     FractionBox[
      SubscriptBox["a", "6"], 
      SubscriptBox["a", "5"]]}]}], "}"}], "}"}]], "Output",
 CellChangeTimes->{{3.7232062229330063`*^9, 3.72320622703496*^9}, {
